   "source": [
    "for i in range(2010,2017):\n",
    "    top10_cause = data[(data['Year']==i)&(data['Cause Name']!='All causes')].groupby('Cause Name')['Deaths'].sum().nlargest(10)\n",
    "    print('the top 10 causes in year {} are'.format(i))\n",
    "    print(top10_cause)"
   ]
